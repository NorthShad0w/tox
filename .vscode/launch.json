  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Local",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "env": {
        "CONF_FILE_PATH": "${workspaceFolder}/config/example.config.yaml"
      },
      "program": "${workspaceFolder}/main.go",
      "args": ["local"]
    },
    {
      "name": "Launch Sever",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "env": {
        "CONF_FILE_PATH": "${workspaceFolder}/config/example.config.yaml"
      },
      "program": "${workspaceFolder}/main.go",
      "args": ["server"]
    }
  ]
}
